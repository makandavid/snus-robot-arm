<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<bindings>
			<customBinding>
				<binding name="HttpsDuplexBinding">
					<reliableSession />
					<compositeDuplex />
					<oneWay />
					<textMessageEncoding />
					<httpsTransport authenticationScheme="Anonymous" />
				</binding>
			</customBinding>
		</bindings>
		<client>
			<endpoint address="https://localhost:54319/RobotArmService.svc"
					  binding="customBinding"
					  bindingConfiguration="HttpsDuplexBinding"
					  contract="RobotArmService.IRobotArmService"
					  name="SecureEndpoint">
				<identity>
					<dns value="localhost" />
				</identity>
			</endpoint>
		</client>
	</system.serviceModel>
</configuration>